{
  "name": "すぐ翻訳",
  "version": "0.1",
  "manifest_version": 2,
  "description": "すぐに日本語に翻訳してくれます",
  "background": {"page": "background.html"},
  "permissions" : ["contextMenus"],
  "content_scripts": [{
     "js": [
      "lib/jquery.min.js", 
      "lib/encode.js",
      "background.js"
    ],
     "matches": ["<all_urls>"]
   }],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}